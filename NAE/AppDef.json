{
  "name": "Blender&reg; 4.0.0",
  "description": "Blender is the free and open source 3D creation suite. It supports the entirety of the 3D pipelineâ€”modeling, rigging, animation, simulation, rendering, compositing and motion tracking, video editing and 2D animation pipeline.",
  "author": "Blender Foundation",
  "classifications": [
    "Rendering"
  ],
  "licensed": true,
  "appdefversion": 2,
  "vault-types": [
    "FILE",
    "BLOCK"
  ],
  "machines": [
    "nc*",
    "ng*",
    "n[2-9]*",
    "n[1-9][0-9]*"
  ],
  "scale_max": 1,
  "commands": {
    "interactive": {
      "path": "/usr/local/scripts/start.sh",
      "interactive": true,
      "desktop": true,
      "description": "Launch interactive Blender in your browser",
      "name": "Blender Interactive",
      "parameters": {}
    },
    "benchmark": {
      "path": "/opt/blender/benchmark/benchmark.sh",
      "interactive": false,
      "description": "Run a benchmark to test a systems CPU and/or GPU(s)",
      "name": "Blender Benchmark",
      "parameters": {
        "-renderFile": {
          "name": "Render Image",
          "type": "selection",
          "required": true,
          "positional": false,
          "description": "Choose which image to render. Check README.md for description of files",
          "values": [
            "Aperture",
            "Ryzen CPU"
          ],
          "mvalues": [
            "Aperture",
            "RyzenGraphic_27"
          ]
        },
        "-enableCPU": {
          "name": "Enable CPU",
          "type": "BOOL",
          "required": true,
          "positional": false,
          "description": "Use CPU durring the rendering.",
          "value": false
        },
        "-disableGPU": {
          "name": "Disable GPU",
          "type": "BOOL",
          "required": true,
          "positional": false,
          "description": "Disable GPU durring the rendering.",
          "value": false
        },
        "-gpucount": {
          "name": "Number of GPUs",
          "type": "CONST",
          "required": true,
          "positional": false,
          "value": "%GPUS%",
          "description": "Use the number of GPUs to determine if hardware OpenGL acceleration is available"
        }
      }
    }
  },
  "image": {
    "type": "image/png",
    "data": ""
  }
}
